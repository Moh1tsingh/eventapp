
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int         @id
  name      String
  class     Class
  event     String
  subevent  Subevent[]  @relation("UserSubevents")
}

model Subevent {
  id        Int         @id @default(autoincrement())
  name      String 
  user      User?       @relation("UserSubevents", fields: [userId], references: [id])
  userId    Int? 
}

enum Class {
  Participant
  Admin
  Onspot
}